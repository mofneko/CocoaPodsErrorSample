source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'
swift_version = '4.2'

target 'SampleFramework' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  swift_version = '4.2'

  pod 'RealmSwift'
  pod 'SwiftIconFont'

end

target 'CocoaPodsErrorSample' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  swift_version = '4.2'

  pod 'Eureka'

end
